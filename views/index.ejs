<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SBA 319 MongoDB API Data</title>
</head>
<body>
    <h1>API Data</h1>

    <h2>Users</h2>
    <ul>
        <% users.forEach(user => { %>
            <li><%= user.username %></li>
        <% }); %>
    </ul>

    <h2>Posts</h2>
    <ul>
        <% posts.forEach(post => { %>
            <li><%= post.title %></li>
        <% }); %>
    </ul>
    <h2>Comments</h2>
    <ul>
        <% comments.forEach(comment => { %>
            <li><%= comment.name %></li>
        <% }); %>
    </ul>
</body>
</html>
